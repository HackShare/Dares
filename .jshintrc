{
    "curly": true,   // true: Require {} for every new block or scope
    "eqeqeq": true,  // true: Require triple equals (===) for comparison
    "forin": true,   // true: This option requires all for in loops to filter object's items.
    "freeze": true,  // true: This options prohibits overwriting prototypes of native objects.,
    "immed": true,   // true: Require immediate invocations to be wrapped in parens e.g. `(function () { } ());`
    "indent": 4,     // 4:    This option enforces specific tab width for your code
    "noarg": true,   // true: Prohibit use of `arguments.caller` and `arguments.callee`
    "noempty": true, // true: This option warns when you have an empty block in your code.
    "nonew": true,   // true: This option prohibits the use of constructor functions for side-effects.
    "undef": true,   // true: Require all non-global variables to be declared (prevents global leaks)
    "unused": true,  // true: This option warns when you define and never use your variables.
    "strict": true,  // true: This option requires all functions to run in ECMAScript 5's strict mode.
    "eqnull": true,  // true: Tolerate use of `== null`
    "expr": true,    // This option suppresses warnings about the use of expressions where normally you would expect to see assignments or function calls.
    "node": true,    // Node.js
    "globals": {
        "after": true,
        "before": true,
        "beforeEach": true,
        "describe": true,
        "it": true
    }
}
